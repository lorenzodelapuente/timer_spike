<p id="notice"><%= notice %></p>

<h1>Http Connections</h1>

<table>
  <thead>
    <tr>
      <th>Toggl api</th>
      <th>Clockify api</th>
      <th>Harvest api</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @http_connections.each do |http_connection| %>
      <tr>
        <td><%= http_connection.toggl_api %></td>
        <td><%= http_connection.clockify_api %></td>
        <td><%= http_connection.harvest_api %></td>
        <td><%= link_to 'Show', http_connection %></td>
        <td><%= link_to 'Edit', edit_http_connection_path(http_connection) %></td>
        <td><%= link_to 'Destroy', http_connection, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Http Connection', new_http_connection_path %>
